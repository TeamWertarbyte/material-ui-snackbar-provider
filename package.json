{
  "name": "material-ui-snackbar-provider",
  "version": "1.5.0",
  "description": "A convenient way to use material-ui's snackbars.",
  "main": "lib/index.js",
  "scripts": {
    "test": "standard && jest",
    "test:unit": "jest",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "build": "babel src -d lib && rm lib/*.test.js && (find src -name \\*.d.ts -exec cp {} lib \\;)",
    "prepublishOnly": "babel src -d lib && rm lib/*.test.js && (find src -name \\*.d.ts -exec cp {} lib \\;)",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeamWertarbyte/material-ui-snackbar-provider.git"
  },
  "keywords": [
    "react",
    "material",
    "material-design",
    "material-ui",
    "snackbar",
    "react-component"
  ],
  "author": "Wertarbyte <kontakt@wertarbyte.com> (https://wertarbyte.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TeamWertarbyte/material-ui-snackbar-provider/issues"
  },
  "homepage": "https://github.com/TeamWertarbyte/material-ui-snackbar-provider#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.47",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.1.0",
    "coveralls": "^3.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^25.2.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "standard": "^14.3.3"
  },
  "peerDependencies": {
    "@material-ui/core": "^1.0.0 || ^3.0.0 || ^4.0.0",
    "react": "^16.3.0 || ^17.0.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "jest": {
    "setupFiles": [
      "./test/shim.js",
      "./test/jestsetup.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/test"
    ],
    "roots": [
      "src",
      "test"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ]
}

{
  "name": "material-ui-snackbar-provider",
  "version": "1.0.0",
  "description": "A convenient way to use material-ui's snackbars.",
  "main": "lib/index.js",
  "scripts": {
    "test": "standard && jest",
    "test:unit": "jest",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "build": "babel src -d lib && rm lib/*.test.js",
    "prepare": "babel src -d lib && rm lib/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeamWertarbyte/material-ui-snackbar-provider.git"
  },
  "keywords": [
    "react",
    "material",
    "material-design",
    "material-ui",
    "snackbar",
    "react-component"
  ],
  "author": "Wertarbyte <kontakt@wertarbyte.com> (https://wertarbyte.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TeamWertarbyte/material-ui-snackbar-provider/issues"
  },
  "homepage": "https://github.com/TeamWertarbyte/material-ui-snackbar-provider#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.9.2",
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^24.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "standard": "^12.0.1"
  },
  "peerDependencies": {
    "@material-ui/core": "^1.0.0 || ^3.0.0",
    "react": "^16.3.0"
  },
  "dependencies": {
    "prop-types": "^15.5.8"
  },
  "jest": {
    "setupFiles": [
      "./test/shim.js",
      "./test/jestsetup.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/test"
    ],
    "roots": [
      "src",
      "test"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib/**/*.js"
  ]
}
